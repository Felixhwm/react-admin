{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name" : [true, "never-prefix"],
    "allowDeclarations": false,
    "allowDefinitionFiles": false,
    "no-namespace": false,
    "no-console": false,
    "ordered-imports": false
  }
}
